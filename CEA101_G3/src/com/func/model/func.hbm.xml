<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.func.model.FuncVO" table="func" >
        
        <id name="func_id" column="func_id"> 
        </id>   
        <property name="func_name" column="func_name" />
        <property name="func_info" column="func_info" />
    
    </class>
</hibernate-mapping>

