<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.emp_func.model.EmpFuncVO" table="emp_func">

		<composite-id>
			<!-- key-property標籤表示哪一些屬性對應複合主鍵 -->
			<key-property name="emp_id" column="emp_id"></key-property>
			<key-property name="func_id" column="func_id"></key-property>
		</composite-id>
		
	</class>
</hibernate-mapping>

